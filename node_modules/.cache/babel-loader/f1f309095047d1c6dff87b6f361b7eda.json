mponentName \" \r\n        :class= \"['button', { active : currentTab === tab.componentName }]\">\r\n            <p>\r\n                <!-- write the name of button -->\r\n                {{tab.btnName}}    \r\n             </p>\r\n        </button>\r\n    </div>\r\n    <!-- using \"<keep alive>\" to keep the \"counter\" of each product while changing between shopping-basket(سبد خرید) and next-shopping-list(ليست خريد بعدی) -->\r\n    <keep-alive>\r\n        <component :is=\"currentTab\" > </component>\r\n    </keep-alive>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport TheVendor from '../TheVendors';\r\nimport Empty from '../empty'\r\n\r\nexport default {\r\n    data() {\r\n        return{\r\n            // the default button is \"shopping-list (ليست خريد)\"\r\n            currentTab : \"TheVendor\",\r\n            tabs : [\r\n                {\r\n                    btnName : \"سبد خرید\",\r\n                    componentName : \"TheVendor\"\r\n                },\r\n                {\r\n                    btnName : \"لیست خرید بعدی\",\r\n                    componentName : \"Empty\"\r\n                }\r\n            ],\r\n        }\r\n    },\r\n    components:{\r\n        TheVendor,\r\n        Empty\r\n    },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.HeaderBtn {\r\n    display: flex; \r\n    align-items: center; \r\n    justify-content: center; \r\n    text-align: center; \r\n    flex-direction: row;\r\n}\r\nbutton {\r\n    text-decoration: none;\r\n    width: 180px; \r\n    height: 40px;  \r\n    line-height: 36px;\r\n    background-color: #ffffff; \r\n    border-top: #ffffff; \r\n    border-left: #ffffff; \r\n    border-right: #ffffff; \r\n    border-bottom: #ffffff;\r\n    outline: none;\r\n    cursor: pointer;\r\n}\r\n\r\np {\r\n    color: #535353;\r\n    font-size: 14px;\r\n    text-decoration: none;\r\n    border-bottom: 2px #DFDFDF solid;\r\n    font-family: iransans;\r\n}\r\n/* the style for the button which is clicked */\r\nbutton.active {\r\n    