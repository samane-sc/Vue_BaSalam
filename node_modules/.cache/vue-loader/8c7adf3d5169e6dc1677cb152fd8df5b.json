\r\n        return{\r\n            counter : 1,\r\n            output : String,\r\n            outputM : String,\r\n        }\r\n    },\r\n    \r\n    // Lifecycle hook\r\n    // mounted() {\r\n    // axios\r\n    //   .get(\"https://jsonplaceholder.typicode.com/todos\")\r\n    //   .then((response) => (this.todos = response.data))\r\n    //   .catch((err) => console.error(err));\r\n    // },\r\n      \r\n    methods: {\r\n        plus() {\r\n            this.counter += 1 ;\r\n        },\r\n        minus() {\r\n            if( this.counter > 1 ){\r\n                this.counter -= 1 ;\r\n            }\r\n        },\r\n        mouseDownPlus(duration){\r\n            this.output = \r\n            setTimeout(() => {\r\n                this.counter += 1;\r\n                this.mouseDownPlus(duration - 50);\r\n            }, duration);\r\n        },\r\n        mouseUPPlus(){\r\n            clearTimeout(this.output);\r\n        },\r\n        mouseDownMinus(duration){\r\n            this.outputM =\r\n            setTimeout(() => {\r\n                if(this.counter>1){\r\n                    this.counter -= 1;\r\n                    this.mouseDownMinus(duration - 50);\r\n                }\r\n            }, duration);\r\n        },\r\n        mouseUPMinus(){\r\n            clearTimeout(this.outputM);\r\n        },\r\n        deleteProduct(id){\r\n            this.todos.splice(id, 1);\r\n        },\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n.header{\r\n    width: 360px;\r\n    background-color: #ffffff; \r\n    margin: auto; \r\n    position: relative;\r\n}\r\n\r\n\r\n.disFlex{\r\n    display: flex; \r\n}\r\n\r\n.disFlexCol{\r\n    flex-direction: column;\r\n}\r\n\r\n.order{\r\n    display: flex; \r\n    flex-direction: row; \r\n    margin-right: 16px; \r\n    margin-top: 15px;\r\n}\r\n\r\n.orderName{\r\n    color: #3E3E3E; \r\n    font-size: 12px; \r\n    margin-right: 16px; \r\n    font-family: iransans;\r\n}\r\n\r\n.orderCost{\r\n    display: flex; \r\n    flex-directi