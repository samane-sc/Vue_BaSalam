t while changing between shopping-basket(سبد خرید) and next-shopping-list(ليست خريد بعدی) -->\n    <keep-alive>\n        <component :is=\"currentTab\" > </component>\n    </keep-alive>\n</div>\n</template>\n\n<script>\n\nimport TheVendor from '@/components/page1/TheVendors';\nimport Empty from '@/components/page1/NextShoppingList'\n\nexport default {\n    components:{\n        TheVendor,\n        Empty\n    },\n    data() {\n        return{\n            // the default button is \"shopping-list (ليست خريد)\"\n            currentTab : \"TheVendor\",\n            // buttons\n            tabs : [\n                {\n                    btnName : \"سبد خرید\",\n                    componentName : \"TheVendor\"\n                },\n                {\n                    btnName : \"لیست خرید بعدی\",\n                    componentName : \"Empty\"\n                }\n            ],\n        }\n    }\n}\n</script>\n\n<style scoped>\n.HeaderBtn {\n    display: flex; \n    align-items: center; \n    justify-content: center; \n    text-align: center; \n    flex-direction: row;\n}\nbutton {\n    text-decoration: none;\n    width: 180px; \n    height: 40px;  \n    line-height: 36px;\n    background-color: #ffffff; \n    border: none;\n    outline: none;\n    cursor: pointer;\n}\n\np {\n    color: #535353;\n    font-size: 14px;\n    text-decoration: none;\n    border-bottom: 2px #DFDFDF solid;\n    font-family: iransans;\n}\n\n/* the style for the button which is clicked */\nbutton.active {\n    border-bottom: 2px #DF3856 solid; \n    color: #DF3856;\n}\n\n</style>"]}]}